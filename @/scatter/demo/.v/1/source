import React from 'react';
import ReactDOM from 'react-dom';
import Scatter from '@/scatter';

const app = document.createElement('div');
document.body.appendChild(app);

const scatterSource = [
  ['Price', 'Meat', 'Vegetable'],
  [23, 167, 8.3],
  [81, 284, 12],
  [91, 413, 4.1],
  [13, 287, 13.5],
];
    
ReactDOM.render(
  (
    <Scatter source={scatterSource} />
  ), app,
);


