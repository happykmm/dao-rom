import React from 'react';
import ReactDOM from 'react-dom';
import Sankey from '@/sankey';

const app = document.createElement('div');
document.body.appendChild(app);

const source = [
  ['Source', 'Target', 'Value'],
  ['a', 'c', 5],
  ['a', 'd', 3],
  ['a', 'e', 3],
  ['b', 'e', 8],
  ['e', 'c', 1],
  ['e', 'f', 2],
];
    
ReactDOM.render(
  (
    <Sankey source={source} />
  ), app,
);


